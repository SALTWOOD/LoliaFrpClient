{
  "runtime": "Net80",
  "defaultVariables": null,
  "documentGenerator": {
    "fromDocument": {
      "url": "openapi.json",
      "output": null,
      "newLineBehavior": "Auto"
    }
  },
  "codeGenerators": {
    "openApiToCSharpClient": {
      "clientBaseClass": null,
      "className": "{controller}Client",
      "operationGenerationMode": "MultipleClientsFromFirstTagAndOperationId",
      "namespace": "LoliaFrpClient.Services",
      "generateClientInterfaces": true,
      "injectHttpClient": true,
      "disposeHttpClient": false,
      "typeStyle": "Poco",
      "jsonLibrary": "SystemTextJson",
      "generateDefaultValues": true,
      "generateDataAnnotations": true,
      "generateOptionalParameters": true,
      "generateOptionalPropertiesAsNullable": true,
      "generateNullableReferenceTypes": true,
      "output": "LoliaFrpClient/Services/Generated/FrpApiClient.g.cs",
      "dateTimeType": "System.DateTimeOffset",
      "arrayType": "System.Collections.Generic.ICollection",
      "arrayInstanceType": "System.Collections.ObjectModel.ObservableCollection",
      "arrayDefaultType": "System.Collections.ObjectModel.ObservableCollection",
      "generateCloneMethod": false,
      "generateJsonConverters": true,
      "generateExceptionClasses": true,
      "exceptionType": "System.Exception",
      "useBaseUrl": true,
      "generateBaseUrlProperty": true,
      "cancelTokenParameter": "System.Threading.CancellationToken",
      "newLineBehavior": "Auto"
    }
  }
}